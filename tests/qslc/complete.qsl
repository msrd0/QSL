# this file is a qsl file intended to contain every syntax element that might occur in
# a qsl file to make sure qsl can parse it and the final result is able to compile

database "test"
charset "utf-8"
usevar

# this table has no primary key! qslc should print a warning
table nopk
- text noindex

# this table contains every possibility of datatypes. note that there are other values
# as minsize possible, but these are the most common.
table datatypes
- uint(64) d_pk !primarykey

- int     d_int
- int(8)  d_int8
- int(16) d_int16
- int(32) d_int32
- int(64) d_int64

- uint     d_uint
- uint(8)  d_uint8
- uint(16) d_uint16
- uint(32) d_uint32
- uint(64) d_uint64

- double d_double

- char(5K) d_char
- text     d_text
- password d_pw
- byte(5M) d_byte
- blob     d_blob

- date     d_date
- time     d_time
- datetime d_datetime

# this table tests foreign keys
table fk
- uint(64) f_pk !primarykey
- text name
- &datatypes.d_pk datatype
